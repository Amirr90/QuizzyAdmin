<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.e.quizzyadmin">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_q"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_q"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ShowQuestionActivity"
            android:label="@string/title_activity_show_question"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".AddNewCategory"
            android:label="@string/title_activity_add_new_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ShowSetsActivity"
            android:label="@string/title_activity_show_sets"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>